
{
  "version": 2,
  "builds": [
    {
      "src": "api/serverless.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
